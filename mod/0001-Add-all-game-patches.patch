+
+internal class ModGUIHelper
+{
+	static ModGUIHelper()
+	{
+		ModGUIHelper.tickerArrowTextures = new Tuple<Texture2D, Texture2D>(ModGUIHelper.getTexture("arrow_left.png", 77, 78), ModGUIHelper.getTexture("arrow_right.png", 77, 78));
